<div class="container">
  <h1 class="text-center my-3">Dashboard</h1>
  <div class="row justify-content-around">
    <div class="col-12 col-md-6 col-lg-4">
      <h2 class="text-center mb-3">My subscriptions</h2>

      <% @user_subscriptions.each do |user_subscription| %>
        <%= user_subscription.subscription.name %>
      <% end %>

      <%= link_to "Add new subscription", new_user_subscription_path, class: "btn btn-primary" %>
    </div>

    <div class="col-12 col-md-6 col-lg-4">
      <h2 class="text-center mb-3">My reminders</h2>
    </div>
  </div>
</div>
